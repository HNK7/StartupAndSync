    <script>counter = 0;
timeout = 800;

mouse4_set = false;
g11_set = false;
g10_set = false;


-- HOLD-BUTTON / 2ND-LAYER MACRO FUNCTIONALITY
-- basically, holding down some of the buttons (like G4, G11) will "mode shift"
-- the rest of the other buttons, making them execute new "second-layer" macros


-- this flag is set when any of the second-layer macros get triggered.
-- it is cleared (set back to false) when the original button being held down is released
FLAG_2ND_LAYER_MACRO_PLAYED = false;


function OnEvent(event, arg)
	OutputLogMessage("Counter: %s\n", counter)
	OutputLogMessage("event = %s, arg = %s\n", event, arg)


	-- set flag mouse4_set, when G4 is held
--	if (event == "MOUSE_BUTTON_PRESSED" and arg == 4) then
--		mouse4_set = true;
--	end
--	if (event == "MOUSE_BUTTON_RELEASED" and arg == 4) then
--		mouse4_set = false;
--	end


	-- ^^^^^^^^^^^^^^^^ SEE END OF FILE






	-- If mouse 4 is currently Held Down		(media play/pause / next)
--	if (event == "MOUSE_BUTTON_PRESSED" and arg == 11 and mouse4_set and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
--		PlayMacro("Play/Pause");
--		FLAG_2ND_LAYER_MACRO_PLAYED = true;
--		OutputLogMessage("\n%s", tostring(FLAG_2ND_LAYER_MACRO_PLAYED))
--	end
--	if (event == "MOUSE_BUTTON_PRESSED" and arg == 10 and mouse4_set and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
--		PlayMacro("Next Track");
--		FLAG_2ND_LAYER_MACRO_PLAYED = true;
--		OutputLogMessage("\n%s", ">>>Macro played: Media Next Track		\n")
--	end

---------------------------------------------------------------------------------------------------------------------



-- G11 : Prev tab in browser	|	media play/pause
	if (event == "MOUSE_BUTTON_PRESSED" and arg == 11) then
		g11_set = true;


		-- + mouse4 modifier: Media Play/Pause
		if (mouse4_set and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
			PlayMacro("Play/Pause");
			FLAG_2ND_LAYER_MACRO_PLAYED = true;
			OutputLogMessage(">>>Macro played: Media PLAY/PAUSE		\n")
		end
	end
	if (event == "MOUSE_BUTTON_RELEASED" and arg == 11) then
		g11_set = false;
		if (FLAG_2ND_LAYER_MACRO_PLAYED == true) then
			FLAG_2ND_LAYER_MACRO_PLAYED = false;
			return
		end

		-- G11 prev tab ON RELEASE
		PlayMacro("Prev tab in browser");
	end



-- G10 : Next tab in browser	|	media next
	if (event == "MOUSE_BUTTON_PRESSED" and arg == 10) then
		g10_set = true;


		-- + mouse4 modifier: Media Next
		if (mouse4_set and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
			PlayMacro("Next Track");
			FLAG_2ND_LAYER_MACRO_PLAYED = true;
			OutputLogMessage("\n%s", ">>>Macro played: Media Next Track		\n")
		end
	end
	if (event == "MOUSE_BUTTON_RELEASED" and arg == 10) then
		g10_set = false;
		if (FLAG_2ND_LAYER_MACRO_PLAYED == true) then
			FLAG_2ND_LAYER_MACRO_PLAYED = false;
			return
		end

		-- G10 next tab ON RELEASE
		PlayMacro("Next tab in browser");
	end



-- G7 : Forward		|	Ctrl+R (Refresh)
	if (event == "MOUSE_BUTTON_RELEASED" and arg == 7 and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
		PlayMacro("Forward");
	end


	-- g11 modifier: Ctrl+R (refresh page)
	if (event == "MOUSE_BUTTON_PRESSED" and arg == 7 and g11_set) then
		PlayMacro("Ctrl + R");
		FLAG_2ND_LAYER_MACRO_PLAYED = true;
	end



-- G4 : task view with G11?
	if (event == "MOUSE_BUTTON_PRESSED" and arg == 4) then
		mouse4_set = true;
		PressKey("RCtrl");
		
		if (g11_set and FLAG_2ND_LAYER_MACRO_PLAYED == false) then
			ReleaseKey("RCtrl");
			PlayMacro("Left Windows + Tab");
			FLAG_2ND_LAYER_MACRO_PLAYED = true;
		end
	end
	if (event == "MOUSE_BUTTON_RELEASED" and arg == 4) then
		mouse4_set = false;
		ReleaseKey("RCtrl");

		--if (FLAG_2ND_LAYER_MACRO_PLAYED == false) then
		--	ReleaseKey("LCtrl");
		--end
	end


	-- g11 modifier: Ctrl+R (refresh page)
--	if (event == "MOUSE_BUTTON_PRESSED" and arg == 4 and g11_set) then
--		ReleaseKey("RCtrl");
--		PlayMacro("Left Windows + Tab");
--		FLAG_2ND_LAYER_MACRO_PLAYED = true;
--		OutputLogMessage("ok Left Win + Tab should've played");
--	end


---------------------------------------------------------------------------------------------------------------------
	OutputLogMessage("End\n")
	counter = counter + 1;
end</script>
